                      "name": "drop_atc_to_checkout",
                      "expression": "`drop_atc_to_checkout`"
                    },
                    {
                      "name": "drop_checkout_to_purchase",
                      "expression": "`drop_checkout_to_purchase`"
                    },
                    {
                      "name": "drop_view_to_atc",
